<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:layout_editor_absoluteY="25dp">

  <ImageButton
      android:id="@+id/puppy1"
      android:layout_width="229dp"
      android:layout_height="227dp"
      android:scaleType="fitXY"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/puppy1"
      tools:visibility="visible" />

  <ImageButton
    android:id="@+id/puppy2"
    android:layout_width="208dp"
    android:layout_height="226dp"
    android:scaleType="fitXY"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="@+id/puppy1"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.0"
    app:srcCompat="@drawable/puppy2" />

  <ImageButton
    android:id="@+id/puppy3"
    android:layout_width="425dp"
    android:layout_height="222dp"
    android:scaleType="fitXY"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.491"
    app:srcCompat="@drawable/puppy3" />

  <ImageButton
      android:id="@+id/puppy4"
      android:layout_width="200dp"
      android:layout_height="241dp"
      android:layout_marginTop="424dp"
      android:scaleType="fitXY"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/puppy6" />

  <ImageButton
      android:id="@+id/puppy5"
      android:layout_width="224dp"
      android:layout_height="244dp"
      android:layout_marginLeft="192dp"
      android:layout_marginStart="192dp"
      android:layout_marginTop="424dp"
      android:scaleType="fitXY"
      app:layout_constraintStart_toStartOf="@+id/puppy4"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/puppy4" />
</androidx.constraintlayout.widget.ConstraintLayout>




